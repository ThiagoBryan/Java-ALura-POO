
public class TestaCaracteres {

	public static void main(String[] args) {
		
		char letra = 'A';
		System.out.println(letra );
		
		char valor = 66;
		System.out.println(valor);
		
		valor = (char) (valor + 1); //Casting
		System.out.println(valor);
		
		String palavra = "Alura";
		System.out.println(palavra);

	}

}
